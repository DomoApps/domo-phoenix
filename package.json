{
  "name": "@domoinc/domo-phoenix",
  "version": "0.5.5",
  "description": "Build beautiful charts using Domo's powerful charting engine",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "scripts": {
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "clear": "rm -rf build",
    "create:lib": "mkdir ./build/main/lib && mkdir ./build/module/lib",
    "copy:phoenix": "npm run create:lib && cp ./src/lib/phoenix.js ./build/main/lib/phoenix.js && cp ./src/lib/phoenix.js ./build/module/lib/phoenix.js",
    "copy:globalPhoenix": "cp ./src/lib/DomoChartNM.nocache.js ./build/main/. && cp ./src/lib/DomoChartNM.nocache.js ./build/module/.",
    "build": "npm run clear && npm run build:main && npm run build:module && npm run copy:phoenix && npm run copy:globalPhoenix"
  },
  "keywords": [],
  "author": {
    "name": "John Woodruff",
    "email": "john.woodruff@domo.com"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^10.12.18",
    "prettier": "^1.15.3",
    "typescript": "^3.2.2"
  }
}
